<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
    <bean id="treeDumpEngine" class="com.swisscom.TreeDumpEngine">
        <constructor-arg ref="ServiceRegistry"/>
        <property name="csvOutputDir">
            <value>C:/tmp/alfresco</value>
        </property>
        <property name="rootNodeListInput">
            <value>C:/tmp/rootnodes.csv</value>
        </property>
    </bean>

    <bean id="webscript.com.swisscom.treedump.get"
          class="com.swisscom.TreeDump" parent="webscript">
        <constructor-arg ref="treeDumpEngine"/>
    </bean>

    <bean id="webscript.com.swisscom.treedump-shutdown.get"
          class="com.swisscom.TreeDumpShutdown" parent="webscript">
        <constructor-arg ref="treeDumpEngine"/>
    </bean>
</beans>